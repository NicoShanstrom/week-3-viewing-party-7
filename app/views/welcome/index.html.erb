<% if current_user %>
    <p>
    <% link_to "Log Out" %>
    </p>
    <p>
    <h3><%= cookies.encrypted[:greeting] %></h3>
    </p>
    <p>
    <section class='existing-users'>
    <p>Existing Users: </p>
    <% @users.each do |user| %>
        <li><%= user.email %></li>
    <% end %>
    </section>
    </p>
<% else %>
    <p>
    <%= link_to "Log In", login_path %>
    </p>
    <p>
    <%= button_to "Create New User", register_path, method: :get %>
    </p>
<%end%>
